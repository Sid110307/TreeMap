{
    "build": {
        "development": {
            "android": {
                "resourceClass": "medium"
            },
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "environment": "development",
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "preview": {
            "channel": "preview",
            "distribution": "internal",
            "environment": "preview"
        },
        "production": {
            "android": {
                "credentialsSource": "local",
                "resourceClass": "medium"
            },
            "autoIncrement": true,
            "channel": "production",
            "environment": "production",
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "cli": {
        "appVersionSource": "remote"
    }
}
